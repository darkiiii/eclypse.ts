{
  "name": "@darki_/eclypse.ts",
  "version": "1.0.0",
  "description": "A discord.js bot framework made for simplicity of use and sustainability.",
  "author": "DarKi_",
  "license": "GPL-3.0",
  "keywords": [
    "discord",
    "typescript",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/darkiiii/discord-framework.git"
  },
  "bugs": {
    "url": "https://github.com/darkiiii/discord-framework/issues"
  },
  "homepage": "https://github.com/darkiiii/discord-framework#readme",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint src --ext .ts,.js,.mjs,.cjs",
    "format": "eslint src --ext .ts,.js,.mjs,.cjs --fix",
    "test": "pnpm build && node ."
  },
  "dependencies": {
    "discord.js": "^14.6.0"
  },
  "peerDependencies": {
    "@darki_/logger": "2.0.1",
    "mariadb": "^3.0.2"
  },
  "devDependencies": {
    "@darki_/logger": "2.0.1",
    "@types/node": "^18.11.3",
    "eslint": "^8.26.0",
    "eslint-config-neon": "^0.1.39",
    "mariadb": "^3.0.2",
    "svelte": "^3.52.0",
    "typescript": "^4.8.4",
    "unbuild": "^1.1.2"
  },
  "packageManager": "pnpm@7.13.4",
  "unbuild": {
    "entries": [
      {
        "builder": "mkdist",
        "format": "esm",
        "ext": "mjs",
        "input": "./src/",
        "outDir": "./dist",
        "declaration": true
      },
      {
        "builder": "mkdist",
        "format": "cjs",
        "ext": "cjs",
        "input": "./src/",
        "outDir": "./dist",
        "declaration": false
      }
    ]
  }
}
